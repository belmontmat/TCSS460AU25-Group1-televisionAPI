{
  "name": "tcss460group1-tvdatabase",
  "version": "0.1.0",
  "description": "A system that allows users to retrieve information on thousands of television shows from our database.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "echo \"No tests configured yet\" && exit 0",
    "dev": "npm run start:dev",
    "docs:generate": "ts-node -r tsconfig-paths/register scripts/generateOpenApiSpec.ts",
    "docs:serve": "npm run dev",
    "postinstall": "npm run build"
  },
  "keywords": [
    "typescript",
    "express",
    "nodejs",
    "api",
    "tcss-460",
    "rest",
    "backend"
  ],
  "contributors": [
    {
      "name": "Mathew Belmont"
    },
    {
      "name": "Preston Sia"
    },
    {
      "name": "Sean Miller"
    },
    {
      "name": "Abdul Hassan"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "highlight.js": "^11.11.1",
    "marked": "^16.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.4.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "github:belmontmat/TCSS460AU25-Group1-televisionAPI"
  },
  "homepage": "https://github.com/belmontmat/TCSS460AU25-Group1-televisionAPI"
}
